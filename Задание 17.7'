Money = input("Введите предполагаемую сумму вклада:")
M = float(Money)
Per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
L = list[float](dict.values(Per_cent))
Deposit = [L[0]*M/100, L[1]*M/100, L[2]*M/100, L[3]*M/100]
i = (max(Deposit))
print("Максимальная сумма, которую вы сможете заработать %s" % i) 
